<?xml version="1.0"?>
<configuration>
  <system.diagnostics>
    <sources>      
      <source name="Default" switchValue="Information">
        <listeners>
          <add name="stdout"/>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="stdout" type="System.Diagnostics.ConsoleTraceListener"/>
    </sharedListeners>
  </system.diagnostics>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
